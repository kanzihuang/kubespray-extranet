[Unit]
Description=dummy extranet nat
After=network.target

[Service]
Type=oneshot
RemainAfterExit=yes

ExecStart=/usr/sbin/iptables -t nat -I POSTROUTING -s {{ dummy_extranet_ip }}/32 -j SNAT --to-source {{ node_default_gateway_source }} -m comment --comment "dummy-extranet.service"
ExecStart=/usr/sbin/iptables -t nat -I OUTPUT -d {{ dummy_extranet_ip }}/32 -j DNAT --to-destination {{ node_default_gateway_source }} -m comment --comment "dummy-extranet.service"

ExecStop=/usr/sbin/iptables -t nat -D POSTROUTING -s {{ dummy_extranet_ip }}/32 -j SNAT --to-source {{ node_default_gateway_source }} -m comment --comment "dummy-extranet.service"
ExecStop=/usr/sbin/iptables -t nat -D OUTPUT -d {{ dummy_extranet_ip }}/32 -j DNAT --to-destination {{ node_default_gateway_source }} -m comment --comment "dummy-extranet.service"

[Install]
WantedBy=multi-user.target
